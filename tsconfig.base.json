{
  "extends": "expo-module-scripts/tsconfig.base",
  "compilerOptions": {
    "outDir": "./build",
    "target": "es2019",
    "module": "commonjs",
    "types": ["node", "jest"],
    "typeRoots": ["./ts-declarations", "./node_modules/@types"],
    "baseUrl": ".",
    "paths": {
      "*": ["ts-declarations/*", "node_modules/*"]
    },
    "strict": true
  }
}
